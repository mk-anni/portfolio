.socials {
  display: flex;
  align-items: center;
}
.socials_item {
  margin-right: 40px;
  &:last-child {
    margin-right: 0;
  }
  &-rounded {
    border: 1px solid #61646b;
    border-radius: 50%;
    width: 50px;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: center;
  }
}
.socials_link {
  width: 31px;
  height: 25px;
  display: block;
}
.socials_icon {
  fill: #414c63;
  &:hover {
    fill: #5500f2;
  }
}
