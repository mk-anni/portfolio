.socials__icon--footer {
  fill: #62656c;
  &:hover {
    fill: #ff9a00;
  }
}
.socials__item--rounded {
  border: 1px solid #61646b;
    border-radius: 50%;
    width: 50px;
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: center;
    &:hover {
      border: #ff9a00 1px solid ;
    }
}
